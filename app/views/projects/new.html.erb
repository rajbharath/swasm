<div class="row">
  <h2 class="title">You are adding a new project</h2>
  <div class="col s12">
    <%= render partial: 'project_form', locals: { project: @project, action_text: 'Create' } %>
  </div>
</div>

<!-- Modal Structure -->
<div id="modal1" class="modal bottom-sheet">
  <div class="modal-content">
    <h4>Choose project location</h4>
    <div class="row">
      <%= text_field_tag :enter_location, params[:enter_location], placeholder: 'Type a location.. Ex. Velachery',  class: "col s10 m6 offset-m2" %>
      <a href="#!" class="modal-action modal-close waves-effect waves-green btn-flat col s2 m2">Done</a>
    </div>
    <div class="row">
      <div id="map" class="col s12 m10 offset-m1" style="height: 300px"></div>
    </div>
  </div>
</div>

